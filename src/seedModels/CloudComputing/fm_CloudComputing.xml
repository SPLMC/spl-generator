<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
	<properties>
		<calculations key="tautology" value="true"/>
		<calculations key="auto" value="true"/>
		<graphics key="legendautolayout" value="true"/>
		<calculations key="redundant" value="true"/>
		<graphics key="showshortnames" value="false"/>
		<graphics key="layout" value="horizontal"/>
		<graphics key="showcollapsedconstraints" value="true"/>
		<graphics key="legendhidden" value="false"/>
		<calculations key="constraints" value="true"/>
		<graphics key="layoutalgorithm" value="1"/>
		<calculations key="features" value="true"/>
	</properties>
	<struct>
		<and abstract="true" mandatory="true" name="IaasCloud">
			<and mandatory="true" name="VirtualMachine">
				<alt mandatory="true" name="PriceModel">
					<feature name="OnDemand"/>
					<feature name="Reserved"/>
					<feature name="Spot"/>
				</alt>
				<and mandatory="true" name="InstanceType">
					<and mandatory="true" name="HardwareConfiguration">
						<and mandatory="true" name="ComputeUnit">
							<alt mandatory="true" name="BusSize">
								<feature name="b32"/>
								<feature name="b64"/>
							</alt>
						</and>
						<feature mandatory="true" name="RamMemory"/>
						<feature mandatory="true" name="Network"/>
					</and>
					<alt mandatory="true" name="FamilyType">
						<feature name="Compute"/>
						<feature name="General"/>
						<alt name="Accelerator">
							<feature name="GPU"/>
							<feature name="IntelPhi"/>
							<feature name="FPGA"/>
						</alt>
						<feature name="Memory"/>
						<feature name="Storage"/>
						<feature name="Shared"/>
					</alt>
					<feature name="InstanceDisk"/>
				</and>
				<and mandatory="true" name="VMI">
					<alt mandatory="true" name="VirtualizationTechnique">
						<feature name="HVM"/>
						<feature name="PVM"/>
					</alt>
					<and mandatory="true" name="OperatingSystem">
						<alt mandatory="true" name="Platform">
							<feature name="Linux"/>
							<feature name="Windows"/>
						</alt>
						<alt name="Architecture">
							<feature name="x32"/>
							<feature name="x64"/>
						</alt>
					</and>
					<feature name="SoftwarePackage"/>
					<alt mandatory="true" name="Region">
						<feature name="Africa"/>
						<feature name="Asia"/>
						<feature name="Australia"/>
						<feature name="NorthAmerica"/>
						<feature name="Europe"/>
						<feature name="SouthAmerica"/>
					</alt>
				</and>
				<and mandatory="true" name="Disk">
					<alt mandatory="true" name="DiskType">
						<feature name="Ephemeral"/>
						<feature name="Persistent"/>
						<feature name="ObjectStore"/>
					</alt>
					<alt mandatory="true" name="DiskTechnology">
						<feature name="SSD"/>
						<feature name="Standard"/>
					</alt>
				</and>
				<feature mandatory="true" name="Zone"/>
				<feature name="Group"/>
			</and>
		</and>
	</struct>
	<constraints>
		<rule>
			<imp>
				<var>InstanceDisk</var>
				<conj>
					<var>DiskType</var>
					<var>DiskTechnology</var>
				</conj>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>SoftwarePackage</var>
				<var>OperatingSystem</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>Disk</var>
				<var>Zone</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>Group</var>
				<var>Zone</var>
			</imp>
		</rule>
	</constraints>
	<featureOrder userDefined="true">
		<feature name="VirtualMachine"/>
